{
    "name": "moleculer-sidecar",
    "version": "0.0.2",
    "description": "Moleculer integration for external services",
    "type": "module",
    "exports": "./build/services/sidecar.service.mjs",
    "bin": "./build/index.js",
    "scripts": {
        "compose:up": "DOPPLER_TOKEN=\"$(doppler configs tokens create --project v2-sidecar --config dev api-v2-sidecar-token --plain --max-age 1m)\" docker compose -f ./docker/docker-compose.yml up",
        "build": "node ./scripts/build.mjs --sourcemap",
        "start": "moleculer-runner-esm --config ./moleculer.config.mjs",
        "start:dev": "tsx ./node_modules/moleculer/bin/moleculer-runner.mjs --config ./moleculer.config.ts --env --hot ./src/index.ts"
    },
    "author": "M.Tregub",
    "license": "MIT",
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/body-parser": "^1.19.5",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.9.4",
        "@types/xml2js": "^0.4.14",
        "classic-level": "1.4.1",
        "esbuild": "^0.20.2",
        "fs-extra": "^11.2.0",
        "node-windows": "1.0.0-beta.8",
        "sqlite3": "^5.1.7",
        "tsx": "^4.2.0",
        "typescript": "^5.3.2"
    },
    "dependencies": {
        "@moleculer/channels": "^0.1.8",
        "@moleculer/lab": "^0.6.4",
        "body-parser": "^1.20.2",
        "cbor-extract": "^2.2.0",
        "cbor-x": "^1.5.8",
        "dotenv": "^16.4.5",
        "kleur": "^4.1.5",
        "level": "^8.0.1",
        "lodash": "^4.17.21",
        "moldecor": "github:mishatre/moldecor",
        "moleculer": "^0.14.33",
        "moleculer-web": "^0.10.7",
        "nats": "^2.19.0",
        "xml2js": "^0.6.2"
    },
    "pkg": {
        "targets": [
            "node20-win-arm64"
        ],
        "outputPath": "dist"
    }
}
