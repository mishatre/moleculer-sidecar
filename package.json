{
    "name": "moleculer-sidecar",
    "version": "0.0.1",
    "description": "Moleculer integration for external services",
    "bin": "./build/index.js",
    "main": "index.ts",
    "scripts": {
        "build": "node ./scripts/build.mjs --sourcemap",
        "start": "tsx ./src/index.ts",
        "start1": "tsx ./src/index.ts --config ./moleculer.config.ts --hot --envfile .env",
        "dev": "tsx ./node_modules/moleculer/bin/moleculer-runner.mjs --config moleculer.config.ts --hot ./src/services/*.service.ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "M.Tregub",
    "license": "ISC",
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/body-parser": "^1.19.5",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.9.4",
        "@types/xml2js": "^0.4.14",
        "classic-level": "1.4.1",
        "fs-extra": "^11.2.0",
        "moleculer-decorators": "^1.3.0",
        "node-windows": "1.0.0-beta.8",
        "reflect-metadata": "^0.2.2",
        "sqlite3": "^5.1.7",
        "ts-node": "^10.9.1",
        "tsx": "^4.2.0",
        "typescript": "^5.3.2"
    },
    "dependencies": {
        "@moleculer/lab": "^0.6.4",
        "body-parser": "^1.20.2",
        "cbor-extract": "^2.2.0",
        "cbor-x": "^1.5.8",
        "dotenv": "^16.4.5",
        "esbuild": "^0.20.2",
        "kleur": "^4.1.5",
        "level": "^8.0.1",
        "lodash": "^4.17.21",
        "moleculer": "^0.14.32",
        "moleculer-web": "^0.10.7",
        "nats": "^2.19.0",
        "xml2js": "^0.6.2"
    },
    "pkg": {
        "targets": [
            "node20-win-arm64"
        ],
        "outputPath": "dist"
    }
}
